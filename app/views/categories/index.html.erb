<h2>Loan Categories</h2>
  <% @categories.each do |category| %>
    <div class="category">
      <p><%= link_to "#{category.name}", category_path(category.slug) %><br>
        <%= LoanRequestCategory.where("category_id = ?", category).count %></p>
    </div>
  <% end %>
<div style="clear:both;"></div>

